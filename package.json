{
  "name": "tsbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "prettier": "@yestheoryfam/prettier-config",
  "scripts": {
    "build:docker": "docker build --force-rm --label yesbot -t yesbot --build-arg=YTF_GRAPHQL_ENDPOINT=http://host.docker.internal:5001/graphql .",
    "build:prepare": "NODE_ENV=production && rm -rf node_modules && yarn install && yarn run tsc",
    "build:run": "docker run --link yesbot-ts_postgres_1 .",
    "codegen": "graphql-codegen",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate",
    "lint": "prettier --check \"src/**/*.(js|ts|json)\" \"__tests__/**/*.(js|ts|json)\"",
    "lint:fix": "prettier --write \"src/**/*.(js|ts|json)\" \"__tests__/**/*.(js|ts|json)\"",
    "postinstall": "yarn run db:generate",
    "start": "env-cmd ts-node-dev --files --respawn src/index.ts",
    "start:prod": "node build/src/index.js",
    "start:prodOld": "env-cmd ts-node src/index.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "tsc": "tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "2.2.0",
    "@prisma/client": "5.6.0",
    "@sentry/node": "7.84.0",
    "axios": "1.6.2",
    "countries-and-timezones": "3.5.2",
    "country-code-emoji": "2.3.0",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "discord.js": "14.14.1",
    "fast-glob": "3.3.2",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "node-cron": "3.0.3",
    "path": "0.12.7",
    "reflect-metadata": "0.1.13",
    "ts-node": "10.9.1",
    "uuid": "9.0.1",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@atmina/local-typescript-operations": "0.8.0",
    "@atmina/only-enum-types": "0.7.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript-graphql-request": "6.0.1",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.10",
    "@types/node": "20.10.4",
    "@types/node-cron": "3.0.11",
    "@types/ws": "8.5.10",
    "@yestheoryfam/prettier-config": "1.0.0",
    "env-cmd": "10.1.0",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "prettier": "3.1.0",
    "prisma": "5.6.0",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.2"
  }
}
