{
  "name": "tsbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "prettier": "@yestheoryfam/prettier-config",
  "scripts": {
    "db:prisma": "npx prisma",
    "db:generate": "npm run db:prisma generate",
    "db:migrate": "env-cmd npm run db:prisma migrate",
    "lint": "prettier --check \"src/!(collections)/**/*.(js|ts|json)\"",
    "lint:fix": "prettier --write \"src/!(collections)/**/*.(js|ts|json)\"",
    "postinstall": "npm run db:generate",
    "start": "env-cmd ts-node-dev --respawn src/index.ts",
    "start:prod": "env-cmd ts-node src/index.ts"
  },
  "prisma": {
    "schema": "node_modules/@yes-theory-fam/database/prisma/schema.prisma"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@yes-theory-fam/database": "0.5.1",
    "axios": "0.21.1",
    "countries-and-timezones": "2.4.0",
    "country-code-emoji": "2.3.0",
    "date-fns": "2.22.1",
    "date-fns-tz": "1.1.4",
    "discord.js": "12.5.3",
    "glob": "7.1.7",
    "path": "0.12.7",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "ts-node": "8.10.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/countries-and-timezones": "2.0.3",
    "@types/glob": "7.1.3",
    "@types/node": "14.17.3",
    "@types/ws": "7.4.5",
    "@yestheoryfam/prettier-config": "1.0.0",
    "env-cmd": "10.1.0",
    "prettier": "2.3.1",
    "prisma": "2.22.1",
    "ts-node-dev": "1.1.6",
    "typegraphql-prisma": "0.14.2",
    "typescript": "4.3.4"
  }
}
