    "@types/ws": "8.5.13",
    "@yestheoryfam/prettier-config": "1.0.0",
    "env-cmd": "10.1.0",
    "prettier": "3.3.3",
    "prisma": "5.22.0",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "vitest": "2.1.5"
