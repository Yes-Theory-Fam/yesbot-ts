{
  "name": "tsbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "prettier": "@yestheoryfam/prettier-config",
  "scripts": {
    "build:docker": "docker build --force-rm --label yesbot -t yesbot --build-arg=YTF_GRAPHQL_ENDPOINT=http://host.docker.internal:5001/graphql .",
    "build:prepare": "NODE_ENV=production && rm -rf node_modules && yarn install && yarn run tsc",
    "build:run": "docker run --link yesbot-ts_postgres_1 .",
    "codegen": "graphql-codegen",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate",
    "lint": "prettier --check \"src/**/*.(js|ts|json)\" \"__tests__/**/*.(js|ts|json)\"",
    "lint:fix": "prettier --write \"src/**/*.(js|ts|json)\" \"__tests__/**/*.(js|ts|json)\"",
    "postinstall": "yarn run db:generate",
    "start": "env-cmd ts-node-dev --files --respawn src/index.ts",
    "start:prod": "node build/src/index.js",
    "start:prodOld": "env-cmd ts-node src/index.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "tsc": "tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "1.5.0",
    "@prisma/client": "4.10.1",
    "axios": "1.3.3",
    "countries-and-timezones": "3.4.1",
    "country-code-emoji": "2.3.0",
    "date-fns": "2.29.3",
    "date-fns-tz": "2.0.0",
    "discord.js": "14.7.1",
    "glob": "8.1.0",
    "graphql": "15.8.0",
    "graphql-request": "5.1.0",
    "node-cron": "3.0.2",
    "path": "0.12.7",
    "reflect-metadata": "0.1.13",
    "ts-node": "10.9.1",
    "uuid": "9.0.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@atmina/local-typescript-operations": "0.6.1",
    "@atmina/only-enum-types": "0.5.0",
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript-graphql-request": "4.5.8",
    "@types/countries-and-timezones": "3.2.2",
    "@types/glob": "8.0.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@types/node-cron": "3.0.7",
    "@types/ws": "8.5.4",
    "@yestheoryfam/prettier-config": "1.0.0",
    "env-cmd": "10.1.0",
    "jest": "29.4.2",
    "jest-mock-extended": "3.0.1",
    "prettier": "2.8.4",
    "prisma": "4.10.1",
    "ts-jest": "29.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  },
  "resolutions": {
    "discord-api-types": "0.37.33"
  }
}
