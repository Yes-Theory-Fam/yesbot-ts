    "@atmina/local-typescript-operations": "0.9.0",
    "@atmina/only-enum-types": "0.8.0",
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/near-operation-file-preset": "3.1.0",
    "@graphql-codegen/typescript-graphql-request": "6.3.0",
